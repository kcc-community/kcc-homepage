{
  "name": "kcc-official-website",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "start": "cross-env  NETWORK=test  env-cmd -f ./.env.development craco start",
    "start:main": "cross-env  NETWORK=main env-cmd -f ./.env.production  craco start",
    "build": "cross-env NETWORK=main  env-cmd -f ./.env.production  craco build",
    "build:ana": "cross-env NETWORK=analysis  env-cmd -f ./.env.production  craco build",
    "build:testnet": "cross-env NETWORK=test env-cmd -f ./.env.testnet.production craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "npx cz",
    "format": "eslint --ext .ts,.tsx src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.4.0",
    "@metamask/logo": "^3.0.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/react-helmet": "^6.1.4",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/walletlink-connector": "^6.2.2",
    "antd": "^4.16.1",
    "authereum": "^0.1.14",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "ethereumjs-abi": "^0.6.8",
    "i18next": "^20.3.1",
    "lodash": "^4.17.21",
    "mojitoswap-sdk": "0.0.2",
    "normalize.css": "^8.0.1",
    "rc-scroll-anim": "^2.7.6",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.10.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "react-transition-group": "^4.4.2",
    "react-use-localstorage": "^3.5.3",
    "redux-localstorage-simple": "^2.4.0",
    "rss-parser": "^3.12.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1",
    "web3": "^1.5.3",
    "web3modal": "^1.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@craco/craco": "^5.8.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-slick": "^0.23.4",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^5.1.12",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.16.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.0",
    "commitizen": "^4.2.1",
    "commitlint": "^12.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "craco-less": "^1.17.1",
    "husky": "^6.0.0",
    "i": "^0.3.6",
    "lint-staged": "^10.5.4",
    "npm": "^7.15.1",
    "prettier": "^2.2.0",
    "react-scripts": "4.0.3",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.1.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-env": "^0.8.0"
  },
  "proxy": "https://rpc-mainnet.kcc.network",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**": [
      "prettier --write .",
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
